<?xml version="1.0" encoding="utf-8"?>
<layer-list android:opacity="opaque"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@color/Splash"/>


    <item android:gravity="fill_horizontal">
        <bitmap
            android:gravity="top"
            android:src="@drawable/colorfull" />

    </item>
    <item
        android:gravity="fill_horizontal"
        >
        <bitmap
            android:src="@drawable/gayhd" />

    </item>

    <item
            android:width="300dp"
            android:height="201dp"
            android:gravity="center"
        >
        <bitmap
            android:src="@drawable/text2145"
            />
    </item>


</layer-list>
